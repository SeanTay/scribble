<h2>Posts</h2>

<ul>
  <% @posts.each do |post| %>
    <li>
      <a href ="/posts/<%= post.id %>">
        <%= post.title %> | <%= link_to "Edit", edit_post_path(post) %>
    </li> | <%= link_to "Delete", post_path(post), method: :delete %>
  <% end %>
</ul>

<%= link_to "Create New", new_post_path %>
